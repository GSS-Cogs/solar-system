PREFIX qb: <http://purl.org/linked-data/cube#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX pmdqb: <http://publishmydata.com/def/qb/>

SELECT DISTINCT ?dimension ?codelist ?codelistlabel
WHERE {
  [] a qb:DataSet ;
     qb:structure/qb:component ?component .

  ?component qb:dimension ?dimension ;
     pmdqb:codesUsed / skos:member / skos:inScheme ?codelist .

  ?codelist rdfs:label ?codelistlabel .
}